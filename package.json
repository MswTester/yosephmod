{
  "name": "yongsan-sexmaster",
  "version": "1.0.0",
  "description": "frida based",
  "main": "dist/main/main.js",
  "scripts": {
    "start": "electron ./dist/main/main.js",
    "dev": "concurrently \"npm run watch:main\" \"npm run watch:renderer\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "npm run clean && npm run build:main && npm run build:renderer && npm run copy-files",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "esbuild src/renderer/index.tsx --outfile=dist/renderer/renderer.js --bundle --loader:.tsx=tsx --loader:.ts=ts",
    "watch:main": "tsc -p tsconfig.main.json --watch",
    "watch:renderer": "esbuild src/renderer/index.tsx --outfile=dist/renderer/renderer.js --bundle --loader:.tsx=tsx --loader:.ts=ts --watch",
    "clean": "rimraf dist",
    "copy-files": "node scripts/copy-files.js",
    "dist": "npm run build && electron-builder",
    "dist:win": "npm run build && electron-builder --win",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MswTester/yongsan-sexmaster.git"
  },
  "keywords": [
    "electron",
    "typescript",
    "react",
    "frida"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MswTester/yongsan-sexmaster/issues"
  },
  "homepage": "https://github.com/MswTester/yongsan-sexmaster#readme",
  "build": {
    "appId": "com.yongsan.sexmaster",
    "productName": "Yongsan SexMaster",
    "files": [
      "dist/**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}"
    ],
    "directories": {
      "output": "release",
      "buildResources": "build"
    },
    "extraResources": [
      {
        "from": "assets",
        "to": "assets",
        "filter": [
          "**/*"
        ]
      }
    ],
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "icon": "build/icon.ico"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Yongsan SexMaster"
    },
    "publish": {
      "provider": "github",
      "owner": "MswTester",
      "repo": "yongsan-sexmaster",
      "token": "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",
      "private": true
    },
    "extraMetadata": {
      "main": "dist/main/main.js"
    }
  },
  "dependencies": {
    "frida": "^16.7.14",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.0",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "esbuild": "^0.20.2",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.33.0",
    "fs-extra": "^11.3.0",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0",
    "wait-on": "^7.2.0"
  }
}
